---
import { getCollection } from 'astro:content'

import Layout from 'layouts/Layout.astro'
import Hero from 'main/Hero.astro'
import Projects from 'main/Projects.astro'
import Bio from 'main/Bio.astro'
import Contact from 'main/Contact/Contact.astro'

const seo = await getCollection('seo')
---

<Layout title={seo[0].data.metaDescription}>
  <Hero />
  <Projects />
  <Bio />
  <Contact />
</Layout>
