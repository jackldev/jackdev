---
interface AstroProp {
  strokeColor: string
  className?: string
}

const { strokeColor = 'currentColor', className } = Astro.props as AstroProp
---

<svg
  width="46"
  height="46"
  viewBox="0 0 65 65"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
  stroke={strokeColor}
  stroke-width="1"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <title>Go Top</title>
