---
import { Image as Images } from 'astro:assets'

interface AstroProps {
  class: string
  src: string
  alt: string
  width: number
  height: number
}

const { class: className, src, alt, width, height } = Astro.props as AstroProps
---

<Images
  class={className}
  src={src}
  alt={alt}
  width={width}
  height={height}
  format="webp"
  quality={80}
  loading="lazy"
/>
